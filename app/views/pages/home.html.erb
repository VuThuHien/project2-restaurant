<section id="top" class="parallax-section">
  <div id="top2">
    <%= render "shared/flash" %>
  </div>
</section>
<section class="section">
  <div class="mk-page-section-wrapper">
    <div id="page-section-19" class="mk-page-section self-hosted full_layout full-width-19 center-y">
      <div class="layer1"></div>
      <div class="background-layer"></div>
      <div class="page-section-content vc-row-fluid mk-grid">
        <div class="mk-padding-wrapper">
          <div class="vc-col-sm-12 column_container height-full">
            <div class="vc-inner vc-row vc-row-fluid">
              <div class="vc-column-container vc-col-sm-6">
                <div class="vc-column-inner">
                  <div class="wpb_wrapper">
                    <section id="gallery-loop-20" class="mk-gallery clearfix">
                      <article class="two-column mk-gallery-item simple-frame">
                        <div class="item-holder">
                          <div class="image-hover-overlay"></div>
                          <%= link_to image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/beefweb.jpg",
                            alt:"img", title: "Roast Beef", class: "mk-lightbox mk-image-lightbox" %>
                          <span class="gallery-inner">
                            <%= image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/beefweb.jpg",
                              title: "Roast Beef", alt: "mg" %>
                          </span>
                        </div>
                      </article>
                      <article class="two-column mk-gallery-item simple-frame">
                        <div class="item-holder">
                          <div class="image-hover-overlay"></div>
                          <%= link_to image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/betroot-rizoto-web.jpg",
                            alt:"img", title:"Beetroot Risotto", class:"mk-lightbox mk-image-lightbox" %>
                          <span class="gallery-inner">
                            <%= image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/betroot-rizoto-web.jpg",
                              title:"Beetroot Risotto", alt:"img" %>
                          </span>
                        </div>
                      </article>
                      <article class="two-column mk-gallery-item simple-frame">
                        <div class="item-holder">
                          <div class="image-hover-overlay"></div>
                          <%= link_to image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/lobster-web.jpg",
                            alt:"img", title:"Lobster", class:"mk-lightbox mk-image-lightbox" %>
                          <span class="gallery-inner">
                            <%= image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/lobster-web.jpg",
                              title:"Lobster", alt:"img" %>
                          </span>
                        </div>
                      </article>
                      <article class="two-column mk-gallery-item simple-frame">
                        <div class="item-holder">
                          <div class="image-hover-overlay"></div>
                          <%= link_to image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/salad-1.jpg",
                            alt:"img", title:"Chicory Salad", class:"mk-lightbox mk-image-lightbox" %>
                          <span class="gallery-inner">
                            <%= image_tag "http://www.roast-restaurant.com/wp-content/uploads/2014/05/salad-1.jpg",
                              title:"Chicory Salad", alt:"img" %>
                          </span>
                        </div>
                      </article>
                      <div class="clearboth"></div>
                    </section>
                  </div>
                </div>
              </div>
              <div class="vc-column-container vc-col-sm-6">
                <div class="vc-column-inner ">
                  <div class="wpb-wrapper">
                    <h2 id="fancy-title-21" class="mk-fancy-title simple-style">
                      <span>OUR MENU</span>
                    </h2>
                    <div class="clearboth"></div>
                    <div id="text-block-22" class="mk-text-block">
                      <p>
                        <span>Chef Stuart Cauldwell &nbsp;has created a menu of dishes using the finest seasonal British produce. Table service throughout for food and drinks.</span>
                      </p>
                      <div class="clearboth"></div>
                    </div>
                    <div id="mk-button-23" class="mk-button-container">
                      <%= link_to t(".view_menu"), dishes_path, target: "_self",
                        class: "mk-button skin-light mk-button--text" %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="clearboth"></div>
      </div>
      <div class="clearboth"></div>
    </div>
  </div>

  <div class="mk-main-wrapper-holder">
    <div class="theme-page-wrapper mk-grid vc_row-fluid">
      <div class="theme-content">
        <div class="vc-row vc_row-fluid">
          <div class="vc-col-sm-12 column_container height-full">
            <div id="padding-24" class="clearfix"></div>
          </div>
        </div>
        <div class="vc-row vc_row-fluid">
          <div class="vc-col-sm-6 column_container height-full">
            <h2 id="fancy-title-25" class="mk-fancy-title simple-style">
              <span>Book Online</span>
            </h2>
            <div class="clearboth"></div>
            <div class="vc-inner vc-row vc_row-fluid">
              <div class="vc-column-container vc-col-sm-6">
                <div class="vc-column-inner ">
                  <div class="wpb_wrapper">
                    <div id="text-block-26" class="mk-text-block">
                      <p><strong>Reservations</strong><br>
                        for reservations &amp; enquiries<br>
                        <span class="tel">010-020-0780</span>
                      </p>
                      <div class="clearboth"></div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="vc-column-container vc-col-sm-6">
                <div class="vc-column-inner">
                  <div class="wpb_wrapper">
                    <div id="text-block-28" class="mk-text-block">
                      <p>
                        <strong>Location</strong><br>
                        123 Delicious Street,<br>San Francisco,<br>CA 10110
                      </p>
                      <div class="clearboth"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div><br>
            <div class="mk-button-order">
              <%= link_to t(".book_table"), "#", target: "_self",
                          class: "mk-button skin-dark mk-button--text" %>
            </div>
          </div>
          <div class="vc-col-sm-6 column_container height-full">
            <div class="mk-image simple-frame inside-image">
              <div class="mk-image-holder">
                <div class="mk-image-inner">
                  <%= image_tag "http://belvedereshantigram.com/wp-content/uploads/2016/08/restaurants1.jpg",
                                class:"lightbox-false", alt:"book2", title:"book2" %>
                </div>
              </div>
              <div class="clearboth"></div>
            </div>
          </div>
        </div>
        <div class="vc-row vc_row-fluid ">
          <div class="vc-col-sm-12 column_container height-full">
            <div id="padding-29" class="clearfix"></div>
          </div>
        </div>
        <div class="clearboth"></div>
        <div class="clearboth"></div>
      </div>
      <div class="clearboth"></div>
    </div>
  </div>
</section>
